{
	"publisher": "mohamed-nouri",
	"repository": {
		"url": "https://github.com/MoNouri97/websocket-client"
	},
	"icon": "images/icon.jpg",
	"name": "websocket-client",
	"displayName": "WebSocket Client",
	"description": "Testing Websocket (SockJs + stompjs) Connections Using VsCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:websocket-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "websocket-sidebar-view",
					"title": "WebSocket",
					"icon": "./media/icon.svg"
				}
			]
		},
		"views": {
			"websocket-sidebar-view": [
				{
					"type": "webview",
					"id": "websocket-sidebar",
					"name": "WebSocket",
					"icon": "./media/icon.svg",
					"contextualTitle": "WebSocket"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "rollup -c && webpack --mode production",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "rollup -c && webpack --mode production --devtool hidden-source-map"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^18.0.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@rollup/plugin-replace": "^2.4.2",
		"@rollup/plugin-typescript": "^8.2.1",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/sockjs-client": "^1.5.0",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"concurrently": "^6.0.2",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.38.1",
		"svelte-check": "^1.5.2",
		"svelte-preprocess": "^4.7.3",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@stomp/stompjs": "^6.1.0",
		"sockjs-client": "^1.5.1"
	}
}
