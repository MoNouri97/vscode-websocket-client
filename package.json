{
	"publisher": "mohamed-nouri",
	"repository": {
		"url": "https://github.com/MoNouri97/websocket-client"
	},
	"icon": "images/icon.jpg",
	"name": "websocket-client",
	"displayName": "WebSocket Client",
	"description": "Testing Websocket (SockJs + stompjs) Connections Using VsCode",
	"version": "0.3.0",
	"engines": {
		"vscode": "^1.77.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:websocket-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "websocket-sidebar-view",
					"title": "WebSocket",
					"icon": "./media/icon.svg"
				}
			]
		},
		"views": {
			"websocket-sidebar-view": [
				{
					"type": "webview",
					"id": "websocket-sidebar",
					"name": "WebSocket",
					"icon": "./media/icon.svg",
					"contextualTitle": "WebSocket"
				}
			]
		},
		"configuration": {
			"title": "WebSocket Client",
			"properties": {
				"websocketClient.serializer": {
					"type": "string",
					"description": "Function to serialize outgoing messages",
					"default": "(text) => text"
				},
				"websocketClient.deserializer": {
					"type": "string",
					"description": "Function to deserialize incoming messages",
					"default": "(data) => data"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "rollup -c && webpack --mode production",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "rollup -c && webpack --mode production --devtool hidden-source-map"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^18.1.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@rollup/plugin-replace": "^2.4.2",
		"@rollup/plugin-typescript": "^8.5.0",
		"@tsconfig/svelte": "^1.0.13",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^8.2.3",
		"@types/node": "^12.20.55",
		"@types/sockjs-client": "^1.5.1",
		"@types/vscode": "^1.77.0",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"concurrently": "^6.5.1",
		"eslint": "^7.32.0",
		"glob": "^7.2.3",
		"mocha": "^8.4.0",
		"rollup": "^2.79.1",
		"rollup-plugin-svelte": "^6.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.58.0",
		"svelte-check": "^1.6.0",
		"svelte-preprocess": "^4.10.7",
		"ts-loader": "^8.4.0",
		"typescript": "^4.9.5",
		"vscode-test": "^1.6.1",
		"webpack": "^5.79.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@stomp/stompjs": "^6.1.2",
		"sockjs-client": "^1.6.1"
	}
}
